<?xml version="1.0"?>
<block>
  <name>GOMX-1 FEC decoder</name>
  <key>ax100_gomx1_decode</key>
  <category>[ax100]</category>
  <import>import ax100</import>
  <make>ax100.gomx1_decode($verbose)</make>
  <param>
    <name>Verbose</name>
    <key>verbose</key>
    <type>bool</type>
     <option>
       <name>Yes</name>
       <key>True</key>
     </option>
     <option>
       <name>No</name>
       <key>False</key>
     </option>
  </param>

  <sink>
    <name>in</name>
    <type>message</type>
  </sink>

  <source>
    <name>out</name>
    <type>message</type>
  </source>
</block>
