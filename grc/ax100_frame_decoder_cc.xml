<?xml version="1.0"?>
<block>
  <name>frame_decoder_cc</name>
  <key>ax100_frame_decoder_cc</key>
  <category>ax100</category>
  <import>import ax100</import>
  <make>ax100.frame_decoder_cc($address)</make>
  <param>
    <name>ZMQ Address</name>
    <key>address</key>
    <type>string</type>
  </param>

  <sink>
    <name>in</name>
    <type>byte</type>
  </sink>

  <source>
    <name>out</name>
    <type>message</type>
    <optional>1</optional>
  </source>
</block>
